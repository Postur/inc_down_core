file = { 
	SOI ~ 
    (command | LINE_SEPARATOR | text )+ ~ 
    EOI 
}
char = {
    !("\"" | "ยง") ~ ANY
    | "\\" ~ ("\"" | "\\" | "/" | "b" | "f" | "n" | "r" | "t")
    | "\\" ~ ("u" ~ ASCII_HEX_DIGIT{4})
}
text = @{char+}
word = {LETTER+}
command = {"ยง"~word~"("~(word |("," ~ word)| command)*~")"}
WHITESPACE = _{ " " | "\t" | "\r" | "\n" }